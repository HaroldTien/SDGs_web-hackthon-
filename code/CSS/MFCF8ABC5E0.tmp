@charset "utf-8";
@keyframes open{
	0%{opacity: 0}
	10%{opacity:0.1}
	25%{opacity:0.25}
	40%{opacity: 0.4}
	50%{opacity: 0.5}
	60%{opacity:0.6}
	75%{opacity:0.75}
	100%{opacity: 1}
}
@keyframes text-blink {
	from{
		opacity: 1;
	}to{
		opacity: 0;
	}
}
@keyframes slidein {
  from {
    padding-left: 100%;
    width: 100%;
	
  }

  to {
    padding-left: 0%;
    width: 55%;
	
  }
}

body{
	margin: 0px;
	background-color:#272833;
	color: white;
	font-size: 20px;
	font-family: 'Noto Sans TC', sans-serif;
	animation: slidein 1s ;
}
a{
	margin-top: 5px;
	padding-right: 5px;
	text-decoration: none;
	color: white;
}
.top{
	background-color: #272835;
	width:100%;
	height: 15px;
	position: absolute; top:0px;
}
.title{
	width: 10%;
	position: absolute; top:0px; left: 0px;
}
.meun{
	z-index: 1;
	width: 20%;
	text-align:right;
	position: absolute; top:0px; right:15px;
	
}
.banner{
	width: 100%;
	height: 800px;
}
.conact{
	z-index: 1;
	width: 50%;
	height: 400px;
	position: absolute; top:20%; right: 20%;left: 30%;bottom: 10%;
}
.img-text{
	font-size: 35px;
	animation: slidein 1s;
	position: absolute;top: 25%;bottom:25%;
	
}
.img-inner{
	font-size:20px;
}
.text-blink{
	animation: text-blink 3s infinite alternate ease-in-out;
}
.img{
	animation: slidein 1s;
	position: absolute; top: 25%;bottom: 25%;right: 25%;left: 25%;
	width: 350px;
	height: 350px;
	z-index: 2;
	border-radius: 100%;
	overflow: hidden;
}
.section-one{
	width:100%;
	height:800px;
}
.go-next{
	background-image: url("../IMG/arrow.svg");
	width:35px;
	height:35px;
	
}